---
import Layout, { type Breadcrumb } from "@/layouts/Layout.astro";
import Timeline from "@/components/Timeline.astro";
import Card from "@/components/Card.astro";
import { projects } from "@/data/projects";

const breadcrumb: Breadcrumb[] = [
  {
    text: "proyek",
  },
];
---

<Layout page={breadcrumb} title="Proyek">
  <Card title="Proyek">
    <Timeline timelineItems={projects} />
  </Card>
</Layout>
